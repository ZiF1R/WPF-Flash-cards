<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Primary">#00a3ff</SolidColorBrush>
    <SolidColorBrush x:Key="Danger">#CE1010</SolidColorBrush>
    <SolidColorBrush x:Key="MainBg">#f8f8f8</SolidColorBrush>
    <SolidColorBrush x:Key="CardBg">#ffffff</SolidColorBrush>
    <SolidColorBrush x:Key="TextColor">#171717</SolidColorBrush>
    <SolidColorBrush x:Key="NavBg">#ffffff</SolidColorBrush>
    <SolidColorBrush x:Key="HoverBg">#F6F6F6</SolidColorBrush>
    <SolidColorBrush x:Key="TextColorOpacity">#999999</SolidColorBrush>
    <ImageBrush x:Key="ProfileIcon" ImageSource="/icons/MenuIcons/Light/Profile.png"/>
    <ImageBrush x:Key="ProfileIconActive" ImageSource="/icons/MenuIcons/Profile_Active.png"/>
    <ImageBrush x:Key="FoldersIcon" ImageSource="/icons/MenuIcons/Light/Folders.png"/>
    <ImageBrush x:Key="FoldersIconActive" ImageSource="/icons/MenuIcons/Folders_Active.png"/>
    <ImageBrush x:Key="SettingsIcon" ImageSource="/icons/MenuIcons/Light/Settings.png"/>
    <ImageBrush x:Key="SettingsIconActive" ImageSource="/icons/MenuIcons/Settings_Active.png"/>
    <BitmapImage x:Key="PlusIcon" UriSource="/icons/plus.png" />
</ResourceDictionary>